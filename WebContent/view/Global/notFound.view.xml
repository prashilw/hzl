<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	controllerName="com.hzl.Controller.Global.notFound">
	<Page
		title="{i18n>notFoundTitle}">
		<content>
		<FlexBox
			alignItems="Center"
			justifyContent="Center">
			<items>
				<Label
					class="sapUiLargeMargin"
					textAlign="Center"
					text="{i18n>notFoundText}" />
			</items>
		</FlexBox>
		</content>
		<footer>
			<Bar>
			</Bar>
		</footer>
	</Page>
</mvc:View>